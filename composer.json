{
    "name": "brunocfalcao/crypto-cube",
    "description": "Nidavellir crypto cube",
    "keywords": ["Crypto"],
    "homepage": "https://github.com/brunocfalcao/crypto-cube",
    "license": "MIT",
    "authors": [{
        "name": "Bruno Falcao",
        "email": "bruno.c.falcao@brunofalcao.me",
        "homepage": "https://www.brunocfalcao.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "brunocfalcao/crypto-commands": "dev-master",
        "brunocfalcao/crypto-cube": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nidavellir\\CryptoCube\\": "src",
            "Nidavellir\\CryptoCube\\Factories\\": "database/factories/",
            "Nidavellir\\CryptoCube\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nidavellir\\CryptoCube\\CryptoCubeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
